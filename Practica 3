#include <iostream>
using namespace std;


int main()
{
	int n;
	cout<<"Escribe un numero: "<<endl;
	cin>>n;
	int i;
	for(i=2 ; i<n ; i=i+1)
	{
	    if ( i%2 !=0 && i%3 !=0 && i%5 !=0 && i%7 !=0)
		    {
			cout<<i<<";";
		    }
		 
		else if (i==2 or i==3 or i==5 or i==7)
		{
		   cout<<i<<";";
		}
	}
		
		

	}
