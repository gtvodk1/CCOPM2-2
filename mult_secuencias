#include <iostream>
using namespace std;
void mproducto()
{
    int r = 0;

    for (int i = 1000; i < 100000; i++)
    {
        for (int j = 1000; j < 100000; j++)
        {
            int a = i * j;
            if (a<10000 && a > r)
            {
                r=a;
            }
        }
    }

    cout << r;

}
int main()
{
    
    mproducto();
    return 0;
    
}
